{
  "name": "dracor-metrics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "DEBUG=dracor-metrics:* nodemon -w ./app.js -w ./routes -w ./bin/dracor-metrics-server ./bin/dracor-metrics-server",
    "start": "node ./bin/dracor-metrics-server",
    "test": "xo"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "jsnetworkx": "^0.3.4",
    "morgan": "~1.9.0"
  },
  "xo": {
    "space": true,
    "semicolon": true,
    "envs": [
      "node"
    ],
    "rules": {
      "capitalized-comments": 0,
      "max-len": 1,
      "new-cap": [
        "error",
        {
          "capIsNewExceptions": [
            "Router"
          ]
        }
      ],
      "space-before-function-paren": [
        "warn",
        "always"
      ]
    }
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "xo": "^0.21.1"
  }
}
